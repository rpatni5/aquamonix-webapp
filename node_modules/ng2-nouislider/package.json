{
  "name": "ng2-nouislider",
  "version": "2.0.0",
  "description": "Angular2 noUiSlider component",
  "keywords": [
    "angular2",
    "ng2",
    "nouislider",
    "slider",
    "range",
    "component",
    "directive"
  ],
  "author": "Tomasz Bak <t.bak@selleo.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tb/ng2-nouislider"
  },
  "bugs": {
    "url": "https://github.com/tb/ng2-nouislider/issues"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "@angular/common": ">=14.x",
    "@angular/core": ">=14.x",
    "nouislider": ">=15.x"
  },
  "packageManager": "pnpm@8.5.1",
  "module": "fesm2015/ng2-nouislider.mjs",
  "es2020": "fesm2020/ng2-nouislider.mjs",
  "esm2020": "esm2020/ng2-nouislider.mjs",
  "fesm2020": "fesm2020/ng2-nouislider.mjs",
  "fesm2015": "fesm2015/ng2-nouislider.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2020": "./esm2020/ng2-nouislider.mjs",
      "es2020": "./fesm2020/ng2-nouislider.mjs",
      "es2015": "./fesm2015/ng2-nouislider.mjs",
      "node": "./fesm2015/ng2-nouislider.mjs",
      "default": "./fesm2020/ng2-nouislider.mjs"
    }
  },
  "sideEffects": false,
  "dependencies": {
    "tslib": "^2.3.0"
  }
}