{
  "name": "simplebar-angular",
  "version": "3.3.2",
  "description": "Angular component for SimpleBar",
  "publishConfig": {
    "directory": "dist"
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "license": "MIT",
  "keywords": [
    "simplebar-angular",
    "angular",
    "simplebar"
  ],
  "dependencies": {
    "simplebar-core": "^1.3.2",
    "tslib": "^2.3.0"
  },
  "peerDependencies": {
    "@angular/common": ">=12.0.0",
    "@angular/core": ">=12.0.0"
  },
  "module": "fesm2022/simplebar-angular.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2022": "./esm2022/simplebar-angular.mjs",
      "esm": "./esm2022/simplebar-angular.mjs",
      "default": "./fesm2022/simplebar-angular.mjs"
    }
  },
  "sideEffects": false
}